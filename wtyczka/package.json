{
  "name": "antitestportal-ultra-plasma",
  "displayName": "AntiTestportal Ultra (Plasma Edition)",
  "version": "11.0.0",
  "description": "Premium Testportal Bypass with Ghost Commander Tech",
  "author": "Midway.gg",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package"
  },
  "dependencies": {
    "@plasmohq/messaging": "^0.7.2",
    "@plasmohq/storage": "^1.15.0",
    "plasmo": "0.90.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-toastify": "^11.0.5",
    "use-async-effect": "^2.2.7"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@types/chrome": "0.0.258",
    "@types/node": "20.11.5",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "prettier": "3.2.4",
    "typescript": "5.3.3"
  },
  "manifest": {
    "host_permissions": [
      "https://*.testportal.pl/*",
      "https://*.testportal.net/*",
      "https://*.testportal.online/*"
    ],
    "permissions": [
      "storage",
      "declarativeNetRequest",
      "browsingData"
    ],
    "web_accessible_resources": [
      {
        "resources": [
          "assets/shield.js"
        ],
        "matches": [
          "*://*/*"
        ]
      }
    ],
    "declarative_net_request": {
      "rule_resources": [
        {
          "id": "ruleset_1",
          "enabled": true,
          "path": "rules.json"
        }
      ]
    }
  }
}