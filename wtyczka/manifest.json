{
  "name": "AntiTestportal Ultra",
  "version": "9.9.0",
  "manifest_version": 3,
  "permissions": [
    "storage",
    "scripting",
    "browsingData",
    "tabs",
    "declarativeNetRequest"
  ],
  "host_permissions": [
    "https://*.testportal.pl/*",
    "https://*.testportal.net/*",
    "https://*.testportal.de/*",
    "https://*.testportal.online/*",
    "https://raw.githubusercontent.com/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "declarative_net_request": {
    "rule_resources": [
      {
        "id": "ruleset_1",
        "enabled": true,
        "path": "rules.json"
      }
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "https://*.testportal.pl/*",
        "https://*.testportal.net/*",
        "https://*.testportal.de/*",
        "https://*.testportal.online/*"
      ],
      "js": [
        "bypass/shield.js"
      ],
      "run_at": "document_start",
      "all_frames": true,
      "world": "MAIN"
    },
    {
      "matches": [
        "https://*.testportal.pl/*",
        "https://*.testportal.net/*",
        "https://*.testportal.de/*",
        "https://*.testportal.online/*"
      ],
      "js": [
        "bypass/bridge.js"
      ],
      "run_at": "document_start",
      "all_frames": true,
      "world": "ISOLATED"
    }
  ],
  "action": {
    "default_popup": "popup/index.html"
  }
}